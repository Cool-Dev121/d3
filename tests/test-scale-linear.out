domain([0, 1]).range([0, 1]):
         -0.5  ->  −0.500
          0.0  ->   0.000
          0.5  ->   0.500
          1.0  ->   1.000
          1.5  ->   1.500

domain([1, 2]).range([0, 1]):
          0.5  ->  −0.500
          1.0  ->   0.000
          1.5  ->   0.500
          2.0  ->   1.000
          2.5  ->   1.500

domain([01/01/1990, 01/01/1991]).range([0, 1]):
   10/20/1989  ->  −0.200
   01/01/1990  ->   0.000
   03/15/1990  ->   0.200
   05/27/1990  ->   0.400
   01/01/1991  ->   1.000
   03/15/1991  ->   1.200

domain([0, 1]).range(["red", "blue"]):
         -0.5  ->  rgb(383,0,-127)
          0.0  ->  rgb(255,0,0)
          0.5  ->  rgb(128,0,128)
          1.0  ->  rgb(0,0,255)
          1.5  ->  rgb(-127,0,383)

domain([0, 1]).range(["red", "blue"]).interpolate(hsl):
         -0.5  ->  #0000ff
          0.0  ->  #ff0000
          0.5  ->  #00ff00
          1.0  ->  #0000ff
          1.5  ->  #ff0000

